{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "outDir": "dist",
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "lib": [
      "es6",
      "dom"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "strict": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "files": [
    "src/visual.ts",
    "src/settings.ts"
  ]
}
